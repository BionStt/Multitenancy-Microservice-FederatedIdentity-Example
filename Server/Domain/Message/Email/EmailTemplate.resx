        body, #backgroundTable {
            background-color: #E7E7E7;
        }

        #templateContainer {
            border: 1px solid #DDDDDD;
            margin-top: 45px;
        }


        #templatePreheader {
            background-color: #FAFAFA;
        }

        .preheaderContent div {
            color: #505050;
            line-height: 100%;
            text-align: left;
        }

            .preheaderContent div a:link, .preheaderContent div a:visited, /* Yahoo! Mail Override */ .preheaderContent div a .yshortcuts /* Yahoo! Mail Override */ {
                color: #336699;
                font-weight: normal;
                text-decoration: underline;
            }


        #templateHeader {
            background-color: #FFFFFF;
            border-bottom: 0;
        }

        .headerContent {
            padding: 0;
            padding-left: 20px;
            text-align: left;
            vertical-align: middle;
            background: #000000;
        }

            .headerContent a:link, .headerContent a:visited, /* Yahoo! Mail Override */ .headerContent a .yshortcuts /* Yahoo! Mail Override */ {
                color: #336699;
                font-weight: normal;
                text-decoration: underline;
            }

        #headerImage {
            height: auto;
        }

        #templateContainer, .bodyContent {
            background-color: #FFFFFF;
        }

            .bodyContent div {
                color: #505050;
                font-family: Arial;
                font-size: 14px;
                line-height: 150%;
                text-align: center;
            }

            .bodyContent a, .bodyContent a:visited {
                font-size: 20px;
                padding: 15px;
                text-decoration: none;
                margin-top: 20px;
                background: #1fce6d;
                color: white;
                border: 3px solid #1fce6d;
                display: block;
            }

            .bodyContent img {
                display: inline;
                height: auto;
            }

        #footer {
            font-size: 14px;
            color: #505050;
        }
